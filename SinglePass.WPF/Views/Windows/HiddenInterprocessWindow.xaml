<Window
    x:Class="SinglePass.WPF.Views.Windows.HiddenInterprocessWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SinglePass.WPF.Views.Windows"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:helpers="clr-namespace:SinglePass.WPF.Helpers"
    Title="{x:Static helpers:Constants.InterprocessWindowName}"
    Width="0"
    Height="0"
    ShowInTaskbar="False"
    ShowActivated="False"
    WindowStyle="None"
    mc:Ignorable="d">
    <Grid />
</Window>
